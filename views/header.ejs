    <script>
     
      function getCookie(name) {
      var match = document.cookie.match(RegExp("(?:^|;\\s*)" + escape(name) + "=([^;]*)"));
      return match ? match[1] : null;
      }

      function checkLogin() {
        if( (getCookie("token") != null && getCookie("token") != 'null') && window.location.pathname == '/login')
        {
          window.location = '/';
        }
        else
        {
            if( (getCookie("token") == null || getCookie("token") == 'null')  && window.location.pathname != '/login')
            {
              window.location = '/login';
            }
        }
    	}
    checkLogin()
    
    </script>
